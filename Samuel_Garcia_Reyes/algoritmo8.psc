Algoritmo ejercicio_8
	definir monto,pollo,gaseosa,ensalada  Como Real
	Escribir "Ingrese cantidad de pollo"
	Leer pollo
	Escribir "Ingrese cantidad de gaseosa"
	Leer gaseosa
	Escribir "Ingrese cantidad de ensalada"
	Leer ensalada
	monto<-(pollo*24)+(gaseosa*4.50)+(ensalada*5)
	Escribir "El monto es: " monto
FinAlgoritmo